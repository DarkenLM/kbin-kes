{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2021,
      "sourceType": "module"
  },
    "env": {
        "browser": true,
        "jquery": true
    },
  "extends": "eslint:recommended",
  "rules": {
      "space-before-function-paren": ["error", "always"],
      "space-before-blocks": ["error", "always"],
      "indent": ["error", 4],
      "max-len": ["warn", 80]
  }
}
